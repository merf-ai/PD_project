, false);\n    },\n    async generateTask() {\n      this.$store.commit('setIsLoading', true);\n      this.processGenerate = 'Генерируется вариант';\n      await axios.post('/test_generator/generate/', {\n        headers: {\n          'Authorization': 'Token ' + this.$store.state.token\n        }\n      }).then(response => {\n        this.profile = response.data;\n        this.processGenerate = 'Вариант сгенерирован';\n        this.$router.push('/account');\n      }).catch(error => {\n        console.log(error);\n        this.processGenerate = 'Ошибка';\n      });\n      this.$store.commit('setIsLoading', false);\n    }\n  }\n};","map":{"version":3,"mappings":";AAiEA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,gBAAgB,EAAE,KAAK;MACvBC,eAAe,EAAE,EAAE;MACnBC,OAAO,EAAE,EAAE;MACXC,IAAI,EAAE,EAAE;MACRC,eAAe,EAAE;IACrB;EACJ,CAAC;EACDC,OAAO,GAAG;IACN,IAAI,CAACC,OAAO,EAAC;EACjB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,YAAY,GAAG;MACjB,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MACvC,MAAMhB,KAAI,CACLiB,IAAI,CAAC,kCAAkC,EAAE;QACtCC,OAAO,EAAE;UAAE,eAAe,EAAE,QAAO,GAAI,IAAI,CAACH,MAAM,CAACI,KAAK,CAACC;QAAM;MACnE,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACZ,eAAc,GAAIY,QAAQ,CAACpB,IAAI,CAACqB,OAAM;MAC/C,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACZ,IAAIA,KAAK,CAACH,QAAQ,EAAE;UAChB,IAAI,CAACZ,eAAc,GAAIe,KAAK,CAACH,QAAQ,CAACpB,IAAI,CAACqB,OAAM;QACrD;QACAG,OAAO,CAACC,GAAG,CAACF,KAAK;MACrB,CAAC;MACL,IAAI,CAACV,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC5C,CAAC;IAEDY,UAAU,GAAG;MACT,IAAIC,QAAO,GAAI,IAAIC,QAAQ,EAAE;MAC7BD,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACtB,IAAI,CAAC;MACtCT,KAAK,CAACiB,IAAI,CAAC,kCAAkC,EACzCY,QAAQ,EACR;QACIX,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACJ,EACH,CAACG,IAAI,CAAC,YAAY;QACfK,OAAO,CAACC,GAAG,CAAC,WAAW,CAAC;MAC5B,CAAC,EACIH,KAAK,CAAC,YAAY;QACfE,OAAO,CAACC,GAAG,CAAC,WAAW,CAAC;MAC5B,CAAC,CAAC;IACV,CAAC;IACDK,gBAAgB,GAAG;MACf,IAAI,CAACvB,IAAG,GAAI,IAAI,CAACwB,KAAK,CAACxB,IAAI,CAACyB,